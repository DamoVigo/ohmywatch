<div class="container_form"> 
  <div class="container_input">
    <%= simple_form_for @watch, html: { class: "form"} do |f| %>
      <%= f.input :model %>
      <%= f.input :photo, as: :file %>
      <%= f.input :price, as: :numeric %>
      <%= f.input :description %>
      <%= f.input :address, input_html: { id: 'watch_address' } %>
      <%= f.button :submit %>
    <% end %>
  </div>
</div>

